  "license": "MIT",
  "bugs": {
    "url": "https://github.com/New-Chat/new.chat-sdk/issues"
  },
  "homepage": "https://github.com/New-Chat/new.chat-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/crypto-js": "^4.1.0",
    "@types/elliptic": "^6.4.14",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "chai": "^4.3.6",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.10"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
